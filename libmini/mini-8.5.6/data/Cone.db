# implicit cone function

func sphere(par x, par y, par z, par t)
   {return(* 2 sqrt(+ sqr(x) sqr(y) sqr(z)));}

func half_sphere(par x, par y, par z, par t)
   {
   if (< z 0) return(1);
   else return(* 2 sqrt(+ sqr(x) sqr(y) sqr(z)));
   }

func cone(par x, par y, par z, par t)
   {
   if (> z 0.25) return(1);
   else return(+ (- sqrt(+ sqr(x) sqr(y)) (/ z 2)) 0.25);
   }

func cylinder(par x, par y, par z, par t)
   {
   if (< z -0.4) return(1);
   if (> z 0.4) return(1);
   return(+ sqrt(+ sqr(x) sqr(y)) 0.4);
   }

main(par x, par y, par z, par t)
   {return(cone(x,y,z,t));}
